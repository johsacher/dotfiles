#* openfoam
of6 () {
module unload ompi #ompi/gcc/1.5.5
module unload intel #intel/2017.2 
module unload pgi #pgi/17.4 
module unload gcc #  

module load gcc/9.2.0
module load ompi/gcc/3.1.3
source ~/OpenFOAM/OpenFOAM-6/etc/bashrc
}


#* environment default
